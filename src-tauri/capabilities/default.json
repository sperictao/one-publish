{
  "identifier": "default",
  "description": "Default capabilities for OnePublish",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-start-dragging",
    "core:window:allow-toggle-maximize",
    "dialog:default",
    "dialog:allow-open",
    "shell:default",
    "shell:allow-execute",
    "shell:allow-spawn",
    "process:default",
    "opener:default"
  ]
}
